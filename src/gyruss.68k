set_active_queue_semaphore:
	move.b	#1,queue_sprites_flag_a702(a6)   
	rts
	
set_inactive_draw_semaphore:
	clr.b	draw_queued_sprites_flag_a701(a6)   
	rts
set_inactive_queue_semaphore:
	clr.b	queue_sprites_flag_a702(a6)   
	rts


read_videoram_a0:
	move.l	a0,-(a7)
	sub.l	#0x8000,a0
	move.b	(a6,a0.w),d0
	move.l	(a7)+,a0
	rts
	
write_videoram_a1:
	exg	a0,a1
	jbsr	osd_w_videoram
	exg	a0,a1
	rts
load_video_ram_d5_to_a0:
	move.w	d5,-(a7)
	sub.w	#0x8000,d5
	.ifndef	RELEASE
	jpl		0f
1:
	add.w	#0x8000,d5
	BREAKPOINT	"wrong RAM address in D5"
	illegal
0:
	cmp.w	#0x800,d5
	jcc		1b
	.endif
	lea		(a6,d5.w),a0
	move.w	(a7)+,d5
	rts
	
